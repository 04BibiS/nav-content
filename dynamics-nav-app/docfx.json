{
	"build": {
		"content": [{
			"files": ["**/**.md",
				"**/**.yml"
			],
			"exclude": ["**/obj/**",
				"**/includes/**",
				"README.md",
				"LICENSE",
				"LICENSE-CODE"
			]
		}],
		"resource": [{
			"files": [
				"**/*.png",
				"**/*.jpg",
				"**/*.gif"
			],
			"exclude": [
				"**/obj/**",
				"**/includes/**"
			]
		}],

		"overwrite": [],
		"externalReference": [],
		"globalMetadata": {
			"searchScope": ["DynamicsNAV"],
			"uhfHeaderId": "MSDocsHeader-Dynamics365",
			"titleSuffix": "Dynamics NAV App"
		},
		"template": "op.html",
		"dest": "articles"
	}
}